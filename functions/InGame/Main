loadstring(game:HttpGet("https://github.com/dazscripts/specter/blob/main/functions/InGame/Lobby"))()

Char = plr:WaitForChild("Character")

DropItem(1)

GetItem("EMF Reader")
GetItem("Spirit Box")
GetItem("Book")

